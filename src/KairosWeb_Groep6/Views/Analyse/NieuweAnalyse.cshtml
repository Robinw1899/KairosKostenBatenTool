@{
    ViewData["title"] = "Nieuwe Analyse";
}

<div>
    @Html.Partial("_Tabs")
</div>

<div class="page-header">
    <h3>@ViewData["title"]</h3>
</div>

<div>
    <form asp-controller="Kairos" asp-action="NieuweOfBestaandeWerkgever" asp-route-returnurl="@ViewData["ReturnUrl"]" method="post">
        <div class="col-md-12">
            <div class="btn-group-lg NieuweOfBestaandeWerkgeverButtons">
                <a asp-controller="Kairos" asp-action="NieuweWerkgever" class="btn btn-info" style="margin-right: 10px;">Nieuwe werkgever</a>
                <a asp-controller="Kairos" asp-action="BestaandeWerkgever" class="btn btn-primary" style="margin-left: 10px;">Bestaande werkgever</a>
            </div>
        </div>
        <br><br><br>
    </form>
</div>