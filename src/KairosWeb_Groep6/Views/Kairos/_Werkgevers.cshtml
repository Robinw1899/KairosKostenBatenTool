@model System.Collections.Generic.IEnumerable<KairosWeb_Groep6.Models.Domain.Werkgever>

<table class="table">
    <tr>
        <th>Naam</th>
        <th>Gemeente</th>
        <th>Straat en Nummer</th>
        <th></th>
        
        
    </tr>
    
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Naam</td>
            <td>@item.Gemeente</td>
            <td>@item.Straat @item.Nummer</td>
            <td>
                <!--Deze action moet normaal naar de navigatie gaan voor baten-->
                <a asp-controller="Kairos" asp-action="NieuweAnalyse" asp-route="@item.WerkgeverId">Nieuwe Analyse</a>
            </td>
           
        </tr>
    }
</table>
